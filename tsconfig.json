{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@shared/*": ["./shared/*"]
    },
    "typeRoots": ["./node_modules/@types"],
    "types": ["jest"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "../shared/**/*.d.ts",
    "./types/**/*.d.ts",
    "./react-native-picker.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
